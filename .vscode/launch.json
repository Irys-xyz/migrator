{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ts-node",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "env": {
                "TS_NODE_COMPILER_OPTIONS": "{\"noUnusedLocals\":false}"
            },
            "runtimeArgs": [
                "--preserve-symlinks",
                "-r",
                "./node_modules/ts-node/register"
            ],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "console": "internalConsole",
            // "outputCapture": "std"
        }
    ]
}